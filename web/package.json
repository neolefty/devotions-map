{
  "name": "devotions-map",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/styles": "^4.10.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.0",
    "@types/react-map-gl": "^5.2.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-map-gl": "^5.2.8",
    "react-scripts": "3.4.3",
    "typescript": "~4.0.3"
  },
  "scripts": {
    "env:prod": "cpy --rename=config.js static/config-prod.js public",
    "env:local": "cpy --rename=config.js static/config-local.js public",
    "env:initial": "cpy --rename=config.js --no-overwrite static/config-local.js public",
    "download": "cd ../data/spreadsheet && ./download.sh && cd ../../web",
    "data": "yarn download && csvtojson ../data/spreadsheet/devotions.csv > public/devotions.json",
    "clean": "rm public/config.js",
    "start": "yarn env:initial && react-scripts start",
    "build": "yarn env:prod && react-scripts build",
    "build-local": "yarn env:local && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cpy-cli": "^3.1.1"
  }
}
